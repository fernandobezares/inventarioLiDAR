/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var mfe_cyl = ee.FeatureCollection("users/cesefor/InventarioForestal/mfe_CastillayLeon"),
    cyl = ee.FeatureCollection("users/cesefor/general/adminUnits/LimiteAdmin_CyL"),
    cyl_prov = ee.FeatureCollection("users/cesefor/general/adminUnits/Provincias_ETRS89");
/***** End of imports. If edited, may not auto-convert in the playground. *****/
// in this script we are exporting featureColelctions to FeatureViews to improve visualization of the vectorial datasets in teh apps 
// mainly Administrative units and Spanish Forest Map 4th version 

// list of collections to turn FeatureView
var collections = [mfe_cyl, cyl, cyl_prov]
var names = ['mfe_cyl', 'cyl', 'cyl_prov']


for (var collection in collections){
  
// fetch collection
var col = collections[collection];
var name = names[collection]
// Export the WDPA FeatureCollection as a FeatureView asset.
Export.table.toFeatureView({
  collection: col,
  assetId: name,
  description: name,
  maxFeaturesPerTile: 1500,
  thinningStrategy: 'HIGHER_DENSITY',
  thinningRanking: ['size DESC'],
  zOrderRanking: ['size DESC'],
});

}